#ArcPy Buffer Around Routes
arcpy.analysis.Buffer(
    in_features="Memorial_Routes_in_Minnesota",
    out_feature_class=r"\\Mac\Home\Documents\ArcGIS\Projects\Lab00\Lab00.gdb\Memorial_Routes_Buffer_Notebook",
